pipeline {
    agent any
stages {
    stage ('RETRIEVE CODE FROM GITHUB') {
        steps {
            sh '''git clone -b $BRANCH https://github.com/hemanthkatkozu/my-repo-db.git
            cp -r $WORKSPACE/my-repo-db/V* $WORKSPACE
            rm -rf $WORKSPACE/my-repo-db'''    
      }
    }
  }
}
